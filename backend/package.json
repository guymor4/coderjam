{
    "name": "coderjam-backend",
    "version": "1.0.0",
    "packageManager": "yarn@4.0.0",
    "description": "Backend server for CoderJam - A web playground that runs code in different languages",
    "main": "dist/server.js",
    "scripts": {
        "dev": "cd ../shared && yarn build && cd ../backend && tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint src --ext .ts --fix",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "@sentry/node": "^10.1.0",
        "coderjam-shared": "link:../shared",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "http-proxy-middleware": "^3.0.3",
        "morgan": "^1.10.0",
        "pg": "^8.13.1",
        "socket.io": "^4.8.1",
        "winston": "^3.11.0",
        "winston-transport": "^4.9.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.0.0",
        "@types/pg": "^8.11.10",
        "@types/socket.io": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^8.38.0",
        "@typescript-eslint/parser": "^8.38.0",
        "eslint": "^9.30.1",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-import": "^2.32.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.1",
        "prettier": "^3.6.2",
        "tsx": "^4.19.2",
        "typescript": "~5.8.3"
    },
    "keywords": [
        "express",
        "typescript",
        "web-playground",
        "code-execution"
    ],
    "author": "",
    "license": "MIT",
    "type": "module"
}
