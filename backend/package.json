{
    "name": "coderjam-backend",
    "version": "1.0.0",
    "description": "Backend server for CoderJam - A web playground that runs code in different languages",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint src --ext .ts --fix",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist"
    },
    "dependencies": {
        "coderjam-shared": "file:../shared",
        "cors": "^2.8.5",
        "express": "^4.21.2",
        "helmet": "^8.0.0",
        "http-proxy-middleware": "^3.0.3",
        "morgan": "^1.10.0",
        "pg": "^8.13.1",
        "socket.io": "^4.8.1"
    },
    "devDependencies": {
        "@types/cors": "^2.8.17",
        "@types/express": "^5.0.0",
        "@types/morgan": "^1.9.9",
        "@types/node": "^22.0.0",
        "@types/pg": "^8.11.10",
        "@types/socket.io": "^3.0.2",
        "@typescript-eslint/eslint-plugin": "^8.36.0",
        "@typescript-eslint/parser": "^8.36.0",
        "eslint": "^9.30.1",
        "tsx": "^4.19.2",
        "typescript": "~5.8.3"
    },
    "keywords": [
        "express",
        "typescript",
        "web-playground",
        "code-execution"
    ],
    "author": "",
    "license": "MIT"
}
